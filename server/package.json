{
  "name": "nationwide.server",
  "scripts": {
    "prestart": "npm run build",
    "start": "NODE_ENV=production node --require dotenv/config dist/server.js",
    "dev": "npm run build && concurrently \"tsc --watch -p tsconfig.prod.json\" \"nodemon --require dotenv/config --ignore data.json dist/server.js\"",
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.prod.json",
    "test": "jest",
    "test:coverage": "npm run test -- --coverage",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix"
  },
  "type": "module",
  "dependencies": {
    "apollo-datasource": "^3.1.0",
    "apollo-server": "^3.3.0",
    "dotenv": "^10.0.0",
    "lowdb": "^3.0.0",
    "uuid": "^8.3.2",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/lodash.clonedeep": "^4.5.6",
    "@types/node": "^16.9.6",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "concurrently": "^6.2.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^24.4.2",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.1",
    "lodash.clonedeep": "^4.5.0",
    "nodemon": "^2.0.12",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.5",
    "ts-jest-resolver": "^1.1.0",
    "typescript": "^4.4.3"
  }
}
