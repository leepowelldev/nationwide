{
  "name": "nationwide",
  "private": "true",
  "workspaces": [
    "client",
    "server"
  ],
  "scripts": {
    "dev": "run-p dev:*",
    "dev:client": "npm run dev -w client",
    "dev:server": "npm run dev -w server",
    "start": "run-p start:*",
    "start:client": "npm run start -w client",
    "start:server": "npm run start -w server",
    "test": "npm run test --workspaces",
    "lint": "npm run lint --workspaces"
  },
  "dependencies": {
    "npm-run-all": "^4.1.5"
  }
}
